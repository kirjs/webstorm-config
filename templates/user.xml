  <template name="fb2" value="const points = s.docs.map(d =&gt; d.data());&#10;&#10;            state.points = points.map(p =&gt; {&#10;                return {...p, min: Math.min(&#10;                    ...points.map(p2 =&gt; distance(p, p2))&#10;                        .filter(d =&gt; d &gt; 0)&#10;                    )};&#10;            }).sort((a,b) =&gt; b.min - a.min);" description="" toReformat="true" toShortenFQNames="true">
